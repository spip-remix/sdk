{
    "name": "spip-remix/sdk",
    "description": "SPIP Development Kit",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "testing"
    ],
    "authors": [
        {
            "name": "JamesRezo",
            "email": "james@rezo.net"
        }
    ],
    "require": {
        "php": ">=8.1",
        "phpunit/phpunit": "^10.5",
        "psr/clock": "^1.0",
        "psr/log": "^3.0",
        "psr/simple-cache": "^3.0",
        "symfony/var-dumper": "^6.4"
    },
    "require-dev": {
        "spip-remix/contracts": "^0.1.x-dev",
        "spip-remix/polyfill-metas": "^0.1.x-dev",
        "spip/filesystem": "^1.2.x-dev"
    },
    "repositories": {
        "spip": {
            "type": "composer",
            "url": "https://get.spip.net/composer/"
        }
    },
    "autoload": {
        "psr-4": {
            "SpipRemix\\Component\\Sdk\\": "src/"
        },
        "files": [
            "src/stub.php"
        ]
    },
    "config": {
        "platform": {
            "php": "8.1.27"
        }
    }
}
